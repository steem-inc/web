{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@zeit-ui/vue": "^2.0.0-alpha.0",
    "core-js": "^2.6.5",
    "dsteem": "^0.9.0",
    "prerender-spa-plugin": "^3.4.0",
    "vue": "^2.6.10",
    "vue-feather-icons": "^5.0.0",
    "vue-i18n": "^8.13.0",
    "vue-router": "^3.1.2",
    "vue-scrollto": "^2.15.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "no-console": 0,
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true
        }
      ],
      "semi": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxBOF": 1,
          "maxEOF": 1
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
